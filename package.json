{
  "name": "eloqua",
  "version": "1.3.5",
  "description": "A node wrapper for the Eloqua API",
  "repository": {
    "type": "git",
    "url": "git://github.com/MadKudu/node-eloqua.git"
  },
  "keywords": [
    "eloqua"
  ],
  "author": {
    "name": "Paul Cothenet",
    "url": "https://github.com/pcothenet/"
  },
  "license": "MIT",
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "postinstall": "",
    "test": "npm run lint && npm run coverage:clean && npm run mocha",
    "coverage:clean": "shx rm -rf ./.nyc_output/ ./coverage/",
    "mocha": "nyc mocha test/*.spec.ts",
    "lint": "tslint --project .",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "dist/",
      "coverage/"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "axios": "^0.27.2",
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/chai": "^4.3.3",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.182",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.23",
    "chai": "^4.3.6",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "shx": "^0.3.4",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.5"
  }
}
