{
  "name": "eloqua",
  "version": "1.3.1",
  "description": "A node wrapper for the Eloqua API",
  "repository": {
    "type": "git",
    "url": "git://github.com/MadKudu/node-eloqua.git"
  },
  "keywords": [
    "eloqua"
  ],
  "author": {
    "name": "Paul Cothenet",
    "url": "https://github.com/pcothenet/"
  },
  "license": "MIT",
  "main": "./dist/client.js",
  "types": "./dist/client.d.ts",
  "files": [
    "dist/**/*"
  ],
  "engines": {
    "node": ">=10.14.1",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "postinstall": "",
    "test": "npm run lint && npm run coverage:clean && npm run mocha",
    "coverage:clean": "shx rm -rf ./.nyc_output/ ./coverage/",
    "mocha": "nyc mocha test/*.spec.ts",
    "lint": "tslint --project .",
    "build": "npm run build:clean && tsc",
    "build:clean": "shx rm -rf ./dist"
  },
  "nyc": {
    "extension": [
      ".js",
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "test/",
      "dist/",
      "coverage/"
    ],
    "reporter": [
      "text",
      "lcov"
    ],
    "all": true
  },
  "dependencies": {
    "axios": "0.19.0",
    "debug": "4.1.1",
    "lodash": "4.17.15"
  },
  "devDependencies": {
    "@types/chai": "4.2.7",
    "@types/debug": "4.1.5",
    "@types/lodash": "4.14.149",
    "@types/mocha": "5.2.7",
    "@types/node": "12.12.17",
    "@types/npm": "2.0.31",
    "chai": "4.2.0",
    "mocha": "6.2.2",
    "nyc": "14.1.1",
    "shx": "0.3.2",
    "ts-node": "8.5.4",
    "tslint": "5.20.1",
    "tslint-config-airbnb": "5.11.2",
    "typescript": "3.7.3"
  }
}
